{
  "name": "express-postgres-api",
  "version": "1.0.0",
  "main": "src/server.js",
  "scripts": {
    "prisma:generate": "prisma generate",
    "prisma:push": "prisma db push && node prisma/seed.js",
    "start": "npm run prisma:push && nodemon --config nodemon.json",
    "start:prod": "npm run prisma:push && node src/server.js",
    "start:container": "npx prisma generate && npm start",
    "dev": "npm run prisma:push && nodemon --config nodemon.json"
  },
  "dependencies": {
    "@faker-js/faker": "^9.7.0",
    "@prisma/client": "^6.6.0",
    "body-parser": "^2.2.0",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "jsonwebtoken": "^9.0.2",
    "bcrypt": "^5.1.1",
    "cors": "^2.8.5",
    "cookie-parser": "^1.4.7"
  },
  "devDependencies": {
    "nodemon": "^3.1.10",
    "prisma": "^6.6.0"
  }
}
